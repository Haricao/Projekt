                    <TextBlock x:Name="Faza" Grid.Column="3" Grid.Row="4" HorizontalAlignment="Center" Height="30" Width="302" RenderTransformOrigin="0.894,0.224" Margin="107,30,41,80" Grid.ColumnSpan="2"   />
                </Grid>

            </TabItem>


