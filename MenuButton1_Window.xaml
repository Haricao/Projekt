                    <!-- Blok 6 TEXT.Napiecie-->

                    <TextBlock x:Name="Ur_C" Grid.Column="3" Grid.Row="3" VerticalAlignment="Center" HorizontalAlignment="Center" Height="35" Width="185" Margin="115,74,0,91" RenderTransformOrigin="0.894,0.224"/>

